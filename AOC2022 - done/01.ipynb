    "def p(d):\n",
    "    d=d.split('\\n\\n')\n",
    "    d=[sum([int(x) for x in c.split()]) for c in d]\n",
    "    d=sorted(d)\n",
    "    return d[-1],sum(d[-3:])\n",
    "\n",
    "a,b=p(open(\"01.txt\").read())\n",
    "print(\"part1:\",a,65912)\n",
    "print(\"part2:\",b,195625)\n",
    "                  \n",
