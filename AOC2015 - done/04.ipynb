    "import hashlib\n",
    "salt = \"bgvyzdsv\"\n",
    "\n",
    "\n",
    "def find(target):\n",
    "    i = 1\n",
    "    while 1:\n",
    "        t=(salt+str(i)).encode('utf-8')\n",
    "        r=hashlib.md5(t).hexdigest()\n",
    "        if r.startswith(target):\n",
    "            print(i,t,r)\n",
    "            break\n",
    "        i+=1\n",
    "find(\"00000\")\n",
    "\n",
    "find(\"000000\")\n"
   ]
